<% provide(:title, "Home") %>
<br/>
<%if !logged_in?%>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
<p align="center">
  <%= button_to "Login to continue", sessions_new_path,:class => "btn btn-success", :method => :get %>
</p>
<br>
<p align="center">
  <%= button_to "Not a Member? Sign Up!", new_user_path,:class => "btn btn-info", :method => :get %>
</p>
</div>
  </div>
    <%else%>
    <br/>
    <h1><span class="label label-primary">Welcome <%= @current_user.name %></span></h1>
<% end %>